{
  "name": "rust-tutorial",
  "dockerComposeFile": [
    "../docker-compose.yml"
  ],
  "workspaceFolder": "/usr/src",
  "service": "rust",
  "settings": {
    "[rust]": {
      "editor.formatOnSave": true,
      "editor.tabSize": 4
    }
  },
  "extensions": [
    "editorconfig.editorconfig",
    "rust-lang.rust-analyzer",
    "christian-kohler.path-intellisense",
    "vadimcn.vscode-lldb"
  ]
}
